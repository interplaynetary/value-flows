{
  "lexicon": 1,
  "id": "vf.proposal.proposalList",
  "defs": {
    "main": {
      "type": "record",
      "description": "A grouping of proposals, for publishing as a list.",
      "key": "tid",
      "record": {
        "type": "object",
        "properties": {
          "proposedTo": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "did"
            },
            "description": "The agent(s) to which the proposal or proposal list is published."
          },
          "lists": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "at-uri"
            },
            "description": "All the proposals included in this proposal list."
          }
        }
      }
    }
  }
}
